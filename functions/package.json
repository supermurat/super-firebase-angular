{
  "name": "functions",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc --project tsconfig.json",
    "serve:beta": "npm run build && firebase serve --project super-murat-beta --only functions",
    "serve:prod": "npm run build && firebase serve --project supermurat-com --only functions",
    "shell:beta": "npm run build && firebase functions:shell --project super-murat-beta",
    "shell:prod": "npm run build && firebase functions:shell --project supermurat-com",
    "deploy:beta": "firebase deploy --project super-murat-beta --only functions",
    "deploy:prod": "firebase deploy --project supermurat-com --only functions",
    "logs:beta": "firebase functions:log --project super-murat-beta",
    "logs:prod": "firebase functions:log --project supermurat-com",
    "test": "jasmine-ts --config=./jasmine.json",
    "test:cover": "del-cli \"./coverage\" && nyc jasmine --config=./jasmine.json",
    "test:cover:open": "npm run test:cover && open-cli \"./coverage/index.html\"",
    "typedoc": "del-cli \"./documentation\" && typedoc --tsconfig tsconfig.json --exclude **/*.spec.ts --name \"super-firebase-angular functions documentation\" --gitRevision master --out documentation ./src",
    "typedoc:open": "npm run typedoc && open-cli \"./documentation/index.html\""
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "8.0.0",
    "firebase-functions": "2.3.1",
    "express": "4.17.1",
    "@angular/animations": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/platform-server": "8.0.0",
    "@angular/pwa": "0.800.1",
    "@angular/router": "8.0.0",
    "@angular/service-worker": "8.0.0",
    "@fortawesome/fontawesome-free": "5.8.2",
    "@google-cloud/storage": "2.5.0",
    "@ng-bootstrap/ng-bootstrap": "4.2.0",
    "@ngx-loading-bar/core": "4.2.0",
    "@ngx-loading-bar/router": "4.2.0",
    "angular2-cookie-law": "^7.0.1",
    "angulartics2": "7.5.2",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "compression": "1.7.4",
    "core-js": "3.1.3",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "firebase": "6.1.0",
    "grpc": "1.21.1",
    "helmet": "3.18.0",
    "helmet-csp": "2.7.1",
    "html2plaintext": "2.1.2",
    "nodemailer": "^6.2.1",
    "protractor": "5.4.2",
    "rxjs": "6.5.2",
    "tslib": "1.9.3",
    "zone.js": "0.9.1",
    "sitemap": "2.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "~3.3.13",
    "@types/node": "~12.0.4",
    "firebase-functions-test": "^0.1.6",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts": "^0.3.0",
    "jquery": "~3.4.1",
    "node-mocks-http": "^1.7.5",
    "nyc": "^14.1.1",
    "popper.js": "~1.15.0",
    "tslint": "~5.17.0",
    "ts-node": "^8.2.0",
    "typedoc": "^0.14.2",
    "typescript": "3.4.5"
  },
  "private": true,
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "lcovonly",
      "text-summary",
      "json-summary",
      "json"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true,
    "check-coverage": true
  }
}
