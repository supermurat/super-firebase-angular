{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "super-firebase-angular": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "functions/dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json"
            ],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "en": {
              "aot": true,
              "outputPath": "functions/dist/browser/en/",
              "baseHref": "/en/",
              "i18nFile": "src/locale/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en-US",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/manifest.json",
                  "with": "src/environments/manifest.en.json"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/environments/index.en.html"
                }
              ]
            },
            "tr": {
              "aot": true,
              "outputPath": "functions/dist/browser/tr/",
              "baseHref": "/tr/",
              "i18nFile": "src/locale/messages.tr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "tr-TR",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/manifest.json",
                  "with": "src/environments/manifest.tr.json"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/environments/index.tr.html"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "serviceWorker": true
            },
            "production-en": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/environments/manifest.en.json"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/environments/index.en.html"
                }
              ],
              "serviceWorker": true,
              "outputPath": "functions/dist/browser/en/",
              "baseHref": "/en/",
              "i18nFile": "src/locale/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en-US",
              "i18nMissingTranslation": "error"
            },
            "production-tr": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/environments/manifest.tr.json"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/environments/index.tr.html"
                }
              ],
              "serviceWorker": true,
              "outputPath": "functions/dist/browser/tr/",
              "baseHref": "/tr/",
              "i18nFile": "src/locale/messages.tr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "tr-TR",
              "i18nMissingTranslation": "error"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "super-firebase-angular:build"
          },
          "configurations": {
            "en": {
              "browserTarget": "super-firebase-angular:build:en"
            },
            "tr": {
              "browserTarget": "super-firebase-angular:build:tr"
            },
            "production": {
              "browserTarget": "super-firebase-angular:build:production"
            },
            "production-en": {
              "browserTarget": "super-firebase-angular:build:production-en"
            },
            "production-tr": {
              "browserTarget": "super-firebase-angular:build:production-tr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "super-firebase-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "sourceMap": true,
            "scripts": [],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "functions/dist/server",
            "main": "src/main-ssr.ts",
            "tsConfig": "src/tsconfig.app-ssr.json"
          },
          "configurations": {
            "en": {
              "outputPath": "functions/dist/server/en",
              "i18nFile": "src/locale/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en-US"
            },
            "tr": {
              "outputPath": "functions/dist/server/tr",
              "i18nFile": "src/locale/messages.tr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "tr-TR"
            },
            "production": {
              "outputPath": "functions/dist/server",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production-en": {
              "outputPath": "functions/dist/server/en",
              "i18nFile": "src/locale/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en-US",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production-tr": {
              "outputPath": "functions/dist/server/tr",
              "i18nFile": "src/locale/messages.tr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "tr-TR",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    },
    "super-firebase-angular-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "super-firebase-angular:serve"
          },
          "configurations": {
            "browserstack": {
              "protractorConfig": "./protractor.browserstack.conf.js"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "super-firebase-angular",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}