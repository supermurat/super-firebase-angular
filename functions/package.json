{
  "name": "functions",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc --project tsconfig.json",
    "serve:beta": "npm run build && firebase serve --project super-murat-beta --only functions",
    "serve:prod": "npm run build && firebase serve --project supermurat-com --only functions",
    "shell:beta": "npm run build && firebase functions:shell --project super-murat-beta",
    "shell:prod": "npm run build && firebase functions:shell --project supermurat-com",
    "deploy:beta": "firebase deploy --project super-murat-beta --only functions",
    "deploy:prod": "firebase deploy --project supermurat-com --only functions",
    "logs:beta": "firebase functions:log --project super-murat-beta",
    "logs:prod": "firebase functions:log --project supermurat-com",
    "test": "jasmine-ts --config=./jasmine.json",
    "test:cover": "del-cli \"./coverage\" && nyc jasmine --config=./jasmine.json",
    "test:cover:open": "npm run test:cover && open-cli \"./coverage/index.html\"",
    "typedoc": "del-cli \"./documentation\" && typedoc --tsconfig tsconfig.json --exclude **/*.spec.ts --name \"super-firebase-angular functions documentation\" --gitRevision master --out documentation ./src",
    "typedoc:open": "npm run typedoc && open-cli \"./documentation/index.html\""
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "8.9.1",
    "firebase-functions": "3.3.0",
    "express": "4.17.1",
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/fire": "5.3.0",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/pwa": "0.803.26",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@google-cloud/storage": "4.7.0",
    "@ng-bootstrap/ng-bootstrap": "5.3.1",
    "@ngx-loading-bar/core": "4.2.0",
    "@ngx-loading-bar/router": "4.2.0",
    "angular2-cookie-law": "^7.0.1",
    "angulartics2": "8.3.0",
    "bootstrap": "4.4.1",
    "classlist.js": "1.1.20150312",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "firebase": "7.14.2",
    "google-auth-library": "5.10.1",
    "grpc": "1.24.2",
    "helmet": "3.22.0",
    "helmet-csp": "2.10.0",
    "html2plaintext": "2.1.2",
    "nodemailer": "^6.4.6",
    "protractor": "5.4.4",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3",
    "sitemap": "6.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.10",
    "@types/node": "~13.13.4",
    "firebase-functions-test": "^0.2.1",
    "jasmine": "^3.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jasmine-ts": "^0.3.0",
    "jquery": "~3.5.0",
    "node-mocks-http": "^1.8.1",
    "nyc": "^15.0.1",
    "popper.js": "~1.16.1",
    "tslint": "5.20.1",
    "ts-node": "^8.9.1",
    "typedoc": "^0.17.6",
    "typescript": "3.5.3"
  },
  "private": true,
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "lcovonly",
      "text-summary",
      "json-summary",
      "json"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true,
    "check-coverage": false
  }
}
